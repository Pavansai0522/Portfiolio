{
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist/portfolio-app/browser",
  "installCommand": "NODE_ENV=development npm install",
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/[...path].js": {
      "maxDuration": 30
    }
  }
}

